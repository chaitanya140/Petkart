<!-- Carousel Start -->
<div class="carousel">
  <div class="container-fluid">
    <div class="owl-carousel">

        <%  carouselTitle.forEach(function(title) {  %>
            
            <div class="carousel-item">
                <div class="carousel-img">
                  <img src="img/<%= title[3]  %> " alt="Image" />
                </div>
                <div class="carousel-text">
                  <h1><%=title[0] %>  <span><%= title[1]  %> </span><%= title[2]  %> </h1>
                  <p>
                    Lorem ipsum dolor sit amet elit. Phasellus ut mollis mauris. Vivamus
                    egestas eleifend dui ac consequat at lectus in malesuada
                  </p>
                  <div class="carousel-btn">
                    <a class="btn custom-btn" href="/login">LogIn</a>
                    <a class="btn custom-btn" href="/signup">SignUp</a>
                      <!-- <form id="log" action="/sign" method="post">
                        
                      <button type="submit" class="btn custom-btn logBtn" name="userChoice" value="logIn">LogIn</button>>
                        <button type="submit" class="btn custom-btn logBtn" name="userChoice" value="signUp">SignUp</button>
                  </form> -->
                  </div>
                </div>
              </div>
        
         <% });  %>  

    </div>
  </div>
</div>
<!-- Carousel End -->
